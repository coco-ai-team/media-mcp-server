{
  "name": "@cocosoft/media-mcp-server",
  "version": "1.0.0-beta.34",
  "author": "Coco AI Team (https://github.com/coco-ai-team)",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "build:debug": "tsc --sourcemap",
    "start": "node dist/index.js",
    "start:sse": "node dist/index.js --sse",
    "docker:build": "docker build --platform linux/amd64 --build-arg ARCH=amd64 -t hxxyyyang/media-mcp-server:$npm_package_version .",
    "docker:push": "docker push hxxyyyang/media-mcp-server:$npm_package_version",
    "docker:publish": "npm run docker:build && npm run docker:push",
    "docker:build:arm64": "docker build --platform linux/arm64 --build-arg ARCH=arm64 -t media-mcp-server:arm64 ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "media-mcp-server": "dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@volcengine/tos-sdk": "^2.7.5",
    "axios": "^1.11.0",
    "commander": "^14.0.0",
    "cuid": "^3.0.0",
    "express": "^5.1.0",
    "ffmpeg-ffprobe-static": "6.1.2-rc.1",
    "fluent-ffmpeg": "^2.1.3",
    "json-schema-to-zod": "^2.6.1",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.0",
    "prettier": "^3.5.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3"
  }
}
